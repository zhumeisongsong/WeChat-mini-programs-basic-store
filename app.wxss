view, image, text {
  box-sizing: border-box;
}

view {
  font-size: 12px;
  color: #333;
}

image {
  display: block;
}

.h1 {
  font-size: 36rpx;
  font-weight: bold;
}

button {
  border: none;
}

.container{
  width:100%;
  height: 100vh;
}

.title{
  padding: 12px;
}

.title .h1{
  display: block;
  font-size: normal;
}

.title .p{
  padding-top: 6px;
}

.flex-center{
  display: flex;
  width: 100vw;
  height: 100vh;
  align-items: center;
  justify-content: center;
  color:#666;
}